  month = jan,
  volume = {12},
  pages = {2064},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/su12052064},
  abstract = {According to the United Nations, cities will represent two-thirds of the world\&rsquo;s population in 2050, which presents some challenges, especially to the transportation sector. To improve the population\&rsquo;s quality of life and the sustainability of cities, mobility must be sustainable, and cycling will play an important role in achieving this. Literature shows that we can promote cycling with better infrastructures, through multimodality, and through behavioral changes. Promoting the use of bicycles through behavior can involve a number of aspects, but in this work, we explore what a digital platform should have in order to promote and increase bicycle usage, as well as to improve cycling conditions in a city. To evaluate what is needed, we conducted an analysis on different types of digital platforms that are available on the market in order to assess the main characteristics and outputs that they provide to cyclists, as well as the type of information that can and should be added to promote the use of bicycles in cities. Moreover, we also carried out a survey in a Portuguese mid-sized \&ldquo;starter cycling city\&rdquo;, which showed, among other data, the relevance and type of information that a digital platform dedicated to cyclists should have in such types of cities.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  file = {C\:\\Users\\desjae\\Zotero\\storage\\WMDTHV2L\\Meireles and Ribeiro - 2020 - Digital PlatformMobile App to Boost Cycling for t.pdf;C\:\\Users\\desjae\\Zotero\\storage\\F8U8C4BE\\2064.html},
  journal = {Sustainability},
  keywords = {Cycling,Digital Platform,Mid-sized Cities,Starter Cycling Cities,Sustainable mobility},
  language = {en},
  number = {5}
}

@article{mertensBuiltEnvironmentalCorrelates2017,
  title = {Built Environmental Correlates of Cycling for Transport across {{Europe}}},
  author = {Mertens, Lieze and Compernolle, Sofie and Deforche, Benedicte and Mackenbach, Joreintje D. and Lakerveld, Jeroen and Brug, Johannes and Roda, C{\'e}lina and Feuillet, Thierry and Oppert, Jean-Michel and Glonti, Ketevan and Rutter, Harry and Bardos, Helga and De Bourdeaudhuij, Ilse and Van Dyck, Delfien},
  year = {2017},
  month = mar,
  volume = {44},
  pages = {35--42},
  issn = {1353-8292},
  doi = {10.1016/j.healthplace.2017.01.007},
  abstract = {This cross-sectional study aimed to determine which objective built environmental factors, identified using a virtual neighbourhood audit, were associated with cycling for transport in adults living in five urban regions across Europe. The moderating role of age, gender, socio-economic status and country on these associations was also investigated. Overall, results showed that people living in neighbourhoods with a preponderance of speed limits below 30km/h, many bicycle lanes, with less traffic calming devices, more trees, more litter and many parked cars forming an obstacle on the road were more likely to cycle for transport than people living in areas with lower prevalence of these factors. Evidence was only found for seven out of 56 possible moderators of these associations. These results suggest that reducing speed limits for motorized vehicles and the provision of more bicycle lanes may be effective interventions to promote cycling in Europe.},
  file = {C\:\\Users\\desjae\\Zotero\\storage\\8V9XWVK9\\Mertens et al. - 2017 - Built environmental correlates of cycling for tran.pdf;C\:\\Users\\desjae\\Zotero\\storage\\L8QXLE3Q\\S1353829217300679.html},
  journal = {Health \& Place},
  keywords = {Active transport,Built environment,Cycling,Google street view,Virtual audit},
  language = {en}
}

