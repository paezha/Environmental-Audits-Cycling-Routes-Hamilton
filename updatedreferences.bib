@article{bornioliPsychologicalWellbeingBenefits2018,
  title = {The Psychological Wellbeing Benefits of Place Engagement during Walking in Urban Environments: {{A}} Qualitative Photo-Elicitation Study},
  shorttitle = {The Psychological Wellbeing Benefits of Place Engagement during Walking in Urban Environments},
  author = {Bornioli, Anna and Parkhurst, Graham and Morgan, Phillip L.},
  year = {2018},
  month = sep,
  volume = {53},
  pages = {228--236},
  issn = {1353-8292},
  doi = {10.1016/j.healthplace.2018.08.018},
  abstract = {The psychological wellbeing potential of walking in urban environments has received limited attention from scholars, despite the important public health implications of identifying characteristics of urban settings that support wellbeing and encourage behaviour change. The study is the first to explore psychological wellbeing experiences of urban walking framed by theories of restorative environments and therapeutic landscape. Self-reported psychological wellbeing experiences of walking in urban settings were investigated with an innovative application of the photo-elicited interview. Fourteen adults took individual walks in Bristol city centre and photographed their journey; photographs were then discussed during the interview. Participants reported specific engagements with place related to personal connections, the identity of place, and sense of community that resulted in psychological wellbeing benefits. The findings also support the notion that non-natural elements can promote positive affective and cognitive appraisals. Building on the finding that also urban walking can support psychological wellbeing, the findings encourage future research into the health potential of different characteristics of built environments.},
  file = {C\:\\Users\\desjae\\Zotero\\storage\\W3TYCQB9\\Bornioli et al. - 2018 - The psychological wellbeing benefits of place enga.pdf;C\:\\Users\\desjae\\Zotero\\storage\\53VP4MJH\\S1353829218302636.html},
  journal = {Health \& Place},
  keywords = {Affective appraisals,Perceived restoration,Urban environments,Walking,Wellbeing},
  language = {en}
}

