@article{harmsPerformanceMunicipalCycling2016a,
  title = {Performance of {{Municipal Cycling Policies}} in {{Medium}}-{{Sized Cities}} in the {{Netherlands}} since 2000},
  author = {Harms, Lucas and Bertolini, Luca and Br{\"o}mmelstroet, Marco Te},
  year = {2016},
